<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="./../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<title>报修任务信息</title>
</head>
<script language="javascript">
	function togetorder(sendcode){
	     document.form1.sendcode.value = sendcode;
	     document.all.form1.action="testorder.aspx";
             document.all.form1.target="saveframe";
             document.all.form1.submit();
	}
	function testok(){
	    thisform.action="updatemesgdatesub.aspx";
            thisform.submit();
	}
</script>
<body>
<form name="thisform" method="post">
   <input type="hidden" name="pageSize" value="$!form_pageSize">
   <input type="hidden" name="page" value="">
<table class="datatable">
  <tr class="trcolor">
    <td colspan="8">      
      <input type="button" name="b2" value="刷&nbsp;&nbsp;新" class="bt1" onclick="javascript:location.reload()"/></td>
  </tr>
  <tr class="trcolor">
    <td>序号</td>
    <td>故障分类</td>
    <td>报修人</td>
    <td>报修问题</td>
    <td>报修日期</td>
    <td>受理日期</td>
    <td>结单日期</td>
    <td>操作</td>
  </tr>
  #set($i=0)
  #foreach($m in $selrecmes)
  <tr class="trcolor">
    <td>$i</td>
    <td align="left">$!m.SENDTYPENAME </td>
    <td align="left">$!m.NAME[ $!m.DETPNAME $!m.MINPHONE ]</td>
    <td align="left">$!m.MARK</td>
    <td align="left" nowrap>$!m.TS</td>
    <td align="left" nowrap>$!m.RECETIME</td>
    <td align="left" nowrap>$!m.ENDTIME</td>
    <td nowrap>#if($!m.RECETIME=="*")<a href="updatemesgdatesub.aspx?sendcode=$!m.SENDCODE" target="saveframe">接单</a>&nbsp;&nbsp<a href="changeprob.aspx?sendcode=$!m.SENDCODE">转让</a>#elseif($!m.ENDTIME=="*")<a href="changeprob.aspx?sendcode=$!m.SENDCODE">转让</a>&nbsp;&nbsp;<a href="receiveprobdetail.aspx?sendcode=$!m.SENDCODE&getusername=$!m.GETUSERNAME">明细</a> #else<a href="receiveprobdetail.aspx?sendcode=$!m.SENDCODE&getusername=$!m.GETUSERNAME">明细</a> #end</td>
  </tr>
  #set($i=$i+1)
  #end
     <tr>
	     <td colspan=8 align="right">$pagecode
	     </td>
   </tr>
</table>
</form>
<form name="form1" method="post">
<input type="hidden" name="sendcode" value="">
</form>
<iframe name="saveframe" src="about:blank" width="100%" height="0" noresize frameborder=0 framespacing=0 marginheight=0 marginwidth=0></iframe> 
</body>
</html>
