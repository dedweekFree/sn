<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<title>======</title>
<script language="javascript">
function ButtonSelectOk(n){
    var ReturnStr="";
        var Checkbox=document.getElementById("Checkbox"+n);
        var unitcode=document.getElementById("unitcode"+n);
        var shenyu=document.getElementById("shenyu"+n);
	var fstyle = document.getElementById("fstyle"+n);
	if(Checkbox.checked){
           ReturnStr1=unitcode.value;
	   ReturnStr2=shenyu.value;
	   ReturnStr3=fstyle.value;
	    window.returnValue=ReturnStr1+"~"+ReturnStr2+"~"+ReturnStr3;
	    window.close();
        }
    }
    function hiddtr(){
	var tab = document.getElementById("TableList");
		for(var i=0;i<tab.rows.length-2;i++){
			var a = document.getElementById("shenyu"+i).value;
			if(a=="0"){
				document.getElementById("A"+i).style.display="none";
			}
		}
    }
</script>
</head>

<body onload="hiddtr()">
<form name="form1" method="post" action="">
<table class="datatable" id="TableList">
  <tr class="trcolor">
    <td colspan="8" align="left">详细信息</td>
  </tr>
   <tr class="trcolor">
    <td>选择</td>
    <td>库位代码</td>
    <td>所处区域</td>
    <td>所处巷道</td>
    <td>所处层</td>
    <td>库位类型</td>
    <td>库位托盘数</td>
    <td>剩余托盘数</td>
  </tr>
  #set($i=0)
  #foreach($!m in $selstockunitcode)
  <tr class="trcolor" #if($!m.FSTYLE==0 && $!m.NNUMBER!=0 ) style="display:none" #else #end id="A$i">
    <td><input type="radio" name="Checkbox" id='Checkbox$i' value="$!m.UNITCODE" onclick="ButtonSelectOk($i)"/></td>
    <td><input type="text" name="unitcode" id="unitcode$i" value="$!m.UNITCODE" style="overflow-x:visible;width:60;" readonly></td>
    <td><input type="text" name="areacode" value="$!m.AREACODE" style="overflow-x:visible;width:60;" readonly><input type="hidden" name="invcode" value="$!m.INVCODE" style="overflow-x:visible;width:60;" readonly></td>
    <td><input type="text" name="pathcode" value="$!m.PATHCODE" style="overflow-x:visible;width:60;" readonly></td>
    <td><input type="text" name="layercode" value="$!m.LAYERCODE" style="overflow-x:visible;width:60;" readonly></td>
    <td><input type="text" name="fstyle" value=" #if($!m.FSTYLE==0) 零托 #else 整托 #end" style="overflow-x:visible;width:60;" readonly>
    <input type="hidden" name="fstyle2" id="fstyle$i"  value="$!m.FSTYLE" readonly>
    </td>
    <td><input type="text" name="uvalue" value="$!m.UVALUE" style="overflow-x:visible;width:60;"readonly></td>
    <td><input type="text" name="shenyu" id="shenyu$i" value="" style="overflow-x:visible;width:60;"readonly>
	<script>
		var a = parseInt($!m.UVALUE);
		var b = parseInt($!m.NNUMBER); 
		var d = parseInt($!m.DTVALUE); 
		if(b==0){
		document.getElementById("shenyu$i").value=$!m.UVALUE;
		}else{
		document.getElementById("shenyu$i").value=a-b/d;
		} 
	</script>
    </td>
  </tr>
#set($i=$i+1)
#end
</table>
</form>
</body>
</html>
