<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<SCRIPT LANGUAGE="JavaScript">
function ShowDate(idText){
 var state="dialogHeight: 300px; dialogWidth: 240px; dialogTop: 200px; dialogLeft: 300px; "+             "edge: Raised;  center: Yes; help: No; resizable: Yes; status: No;";

 var reValue=window.showModalDialog('../../../js/date.html?OpenForm',"myDate",state);
 if(reValue==null) 
   ;
 else
   idText.value=reValue;
 
 }
</SCRIPT>
<title>招标信息</title>

</head>

<body>
<form name="form1" action="tendersub.aspx" method="post">
<table class="datatable">
  <tr class="trcolor">
    <td colspan="1" align="right">项目编号:</td>
    <td align="left"><input type="text" name="proid" /></td>
    <td colspan="2" align=""right>项目名称:<input type="text" name="proname" />
    <input type="hidden" name="prostatus" value="A">
    </td>
  </tr>
  <tr class="trcolor">
    <td align="right">项目类型:</td>
    <td align="left">
	<select name="tendtypeid">
	#set($i=0)
	#foreach($m in $tendtype)
	<option value="$m.TENDTYPEID">$m.TENDTYPE</option>
	#set($i=$i+1)
	#end
	</select>
    </td>
    <td colspan="2">	  
      项目模式:
        <input name="status" type="radio" value="A" checked="checked" />
      普通模式   
       <input type="radio" name="status" value="B" />
    竞价模式 </td>
  </tr>
  <tr class="trcolor">
    <td align="right">开始时间:</td>
    <td align="left"><input type="text" name="atime" id="atime" onclick="ShowDate(atime)"/></td>
    <td>结束时间:        
      <input type="text" name="ztime"  id="ztime" onclick="ShowDate(ztime)"/></td>
    <td>开标时间:<input type="text" name="ktime"  id="ktime" onclick="ShowDate(ktime)"/></td>
  </tr>
  <tr class="trcolor">
    <td align="right">项目说明:</td>
    <td colspan="3" align="left"><textarea name="prodesc" style="width: 100%; height: 100px;"></textarea></td>
  </tr>
  <tr class="trcolor">
    <td align="right">项目附件:</td>
    <td colspan="3"><div id="Flashindexinfo">
                   </div></td>
  </tr>
  <tr class="trcolor">
    <td colspan="4">
      <input type="submit" name="b1" value="保&nbsp;&nbsp;存" /><input type="reset" name="b2" value="重&nbsp;&nbsp;置" /></td>
  </tr>
</table>
</form>
</body>
</html>
