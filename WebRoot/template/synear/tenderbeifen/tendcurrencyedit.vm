<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<title>币种修改</title>
</head>
<body>
<form name="form1" method="post" action="tendcurrencysave.aspx" target="dataframe">
<input type="hidden" name="currencyid" value="$form_currencyid">
<table class="datatable">
  <tr class="trcolor">
    <td align="left">币种修改</td>
  </tr>
  #foreach($!m in $querytendcurrencybyid)
  <tr class="trcolor">
    <td>币种名称:
    <input type="text" name="typename" value="$!m.TYPENAME" >
    </td>
  </tr>
  <tr class="trcolor">
    <td>币种代码:
    <input type="text" name="typecode" value="$!m.TYPECODE" >
    </td>
  </tr>
  <tr class="trcolor">
    <td>币种符号:
    <input type="text" name="typesign" value="$!m.TYPESIGN" >
    </td>
  </tr>
  #end
  <tr class="trcolor">
    <td>      
      <input type="submit" name="b1" value="提&nbsp;&nbsp;交" class="bt1"/>
      <input type="button" name="b2" value="返&nbsp;&nbsp;回" class="bt1" onclick="document.location='tendercurrency.aspx'"/></td>
  </tr>
</table>
    </div>
    <iframe name="dataframe" src="" width="0" height="0"></iframe>
    </div>
</body>
</html>
