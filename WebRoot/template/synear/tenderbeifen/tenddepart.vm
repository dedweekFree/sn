<script src="../../../inc/js/pubjs.js"></script>
<script src="../../../inc/js/preAfter.js"></script>
<title>项目类型</title>
<script language="javascript">
	function s(){
	        if(form1.ptypename.value == ""){
			alert("部门分类不能为空");
			click_on_tree_selector($F('ptypename'),$F('dm_zzjg_tree_layer'));
			return false;
		}
		if(form1.pname.value == ""){
			alert("部门名称不能为空");
			form1.pname.focus();
			return false;
		}
		form1.submit();
	}
	function todelete(url){
	    if(confirm("确定要删除吗？")){
                 document.form1.action=url;
		 form1.submit();
	    }
	}
    function setValDepartment(dm,mc,code) { 
      setValDepart(dm,mc);
      form1.ptypeid.value=dm;
      form1.ptypename.value=mc;
      form1.ptype.value=code;
    }
</script>
</head>

<body>
<form name="form1" method="post" action="insertenddepart.aspx" target="dataframe">
<table class="datatable">
  <tr class="search"> 
	<td class="tdlabel" width="10%">部门分类</td>
	<td class="tdinput" width="20%">
		<input type="text" name="ptypename" value="$!form_ptypename" readonly>
		<input type="hidden" name="ptype" value="$!form_ptype">
		<input type="hidden" name="ptypeid" value="$!form_ptypeid">
		<input type='button' name=bt class="bt1" value='选择' onclick="click_on_tree_selector_depart($F('ptypename'),$F('dm_zzjg_tree_layer'))">
		<div class="dtree" id="dm_zzjg_tree_layer" style="position:absolute;width:300px;height:208px;visibility:Hidden;z-index:1"> 
		<table width="100%" border="1" cellspacing="0" cellpadding="0" height="100%"><tr><td>
			<IFRAME style="width:100%;height:100%" name="swjgdm_tree_frame" marginWidth=0 marginHeight=0 src="depart_tree.aspx" frameBorder=0 
				width="100%" height="100%" scrolling=auto BORDERCOLOR="#000000" ></IFRAME>
		</td></tr></table>
		</div>
		<script>
		   selector_layers[selector_layers.length]=document.all.dm_zzjg_tree_layer;
		</script>				
	</td>	
	<td class="tdlabel" width="10%">部门名称</td>
	<td class="tdinput" width="20%">
		<input type="text" name="pname" value="$!form_pname">		
	</td>
	</tr>
   <tr>
    <td colspan="4" align="right"><input type="button" name="button" class="bt1" value="提交" onclick="s()"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    </td>
  </tr>
  </table>
  </form>
  <table class="datatable">
  <tr class="trcolor">
  <form name="thisform" method="post">
  <input type="hidden" name="pageSize" value="$!form_pageSize">
   <input type="hidden" name="page" value=""></form>
    <td>类别名称</td>
    <td>类别ID</td>
    <td>操作</td>
  </tr>
  #set($i=0)
  #foreach($!m in $seltenddepartgetpage)
  <tr>
    <td align="center">$!m.DEPARTNAME</td>
    <td align="center">$!m.CHILDID</td>
    <td align="center"><a href="tenddepartedit.aspx?departid=$!m.ID">修改</a>&nbsp;&nbsp;&nbsp;<a href="#"  onclick="todelete('tenddepartdel.aspx?id=$!m.CHILDID')">删除</a></td>
  </tr>
  #set($i=$i+1)
  #end
  <tr >
        <td class="tdlabel td" colspan="3">$pagecode</td>
   </tr>
</table>
    <div>
    <iframe name="dataframe" src="" width="0" height="0"></iframe>
    </div>
</body>
</html>
