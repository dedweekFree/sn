<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<title>投标状态</title>
</head>
<script language="javascript">
   function toreturn(){
	history.back(-1);
   }
</script>
<body>
<table class="datatable">
  <tr class="trcolor">
  <form name="thisform" method="post">
  <input type="hidden" name="proid" value="$form_proid">
  <input type="hidden" name="pageSize" value="$!form_pageSize">
   <input type="hidden" name="page" value=""></form>
    <td colspan="10" align="left">投标状态</td>
    <td colspan="3">
	<input type="button" value="返回" class='bt1' onclick="toreturn()">
    </td>
  </tr>
  <tr class="trcolor">
    <td>项目ID</td>
    <td>投标人</td>
    <td>项目名称</td>
    <td>投标价</td>
    <td>招标价</td>
    <td>招标地区</td>
    <td>投标量</td>
    <td>招标量</td>
    <td>单位</td>
    <td>方案</td>
    <td>备注</td>
    <td>状态</td>
    <td>第几轮</td>
  </tr>
  #set($i=1)
  #foreach($!m in $selendershebeiinfo2)
	  <tr>
	    <td align="center">$!m.PROID</td>
	    <td align="center">$!m.COMPANY</td>
	    <td align="center">$!m.PRONAME</td>
	    <td>$!m.TENDPRICE元</td>
	    <td>$!m.ZBPRICE元</td>
	    <td align="center">#if($!m.AREANAME == 'synear')&nbsp;#else$!m.AREANAME#end</td>
	    <td>$!m.QUANTITY</td>
	    <td>$!m.TENDQUANTITY</td>
	    <td align="center">$!m.TYPENAME</td>
	    <td align="center">
	       #if($!m.TENDSTATUS=='K') 
	       <font color="red">中标</font>
               #else
	          未中标
	       #end
            </td>
	    <td align="center">$!m.PROJECT</td>
	    <td align="center">$!m.TENDBAK</td>
	    <td align="center">第[$!m.TENDNUM]轮</td>
	  </tr>
  #set($i=$i+1)
  #end
  <tr >
        <td class="tdlabel td" colspan="13">$pagecode</td>
   </tr>
</table>
</body>
</html>
