<title>成品调拨单</title>
<script type="text/javascript">
var stmp = "";
function UPP(strg)
{
  var number = Math.round(strg*100)/100;
  number = number.toString(10).split(".");
  var a = number[0];
  if (a.length > 12)
    return "数值超出范围！支持的最大数值为 999999999999.99";
  var e = "零壹贰叁肆伍陆柒捌玖";
  var num1 = "";
  var len = a.length-1;
  for (var i=0 ; i<=len; i++)
   num1 += e.charAt(parseInt(a.charAt(i))) + [["件","万","亿"][Math.floor((len-i)/4)],"拾","佰","仟"][(len-i)%4];
  if(number.length==2 && number[1]!="")
  {
    var a = number[1];
    for (var i=0 ; i<a.length; i++)
    num1 += e.charAt(parseInt(a.charAt(i))) + ["角","分"][i]; 
  }
  num1 = num1.replace(/零佰|零拾|零仟|零角/g,"零");
  num1 = num1.replace(/零{2,}/g,"零");
  num1 = num1.replace(/零(?=件|万|亿)/g,"");
  num1 = num1.replace(/亿万/,"亿");
  num1 = num1.replace(/^件零?/,"");

  if(num1!="" && !/分$/.test(num1))
    num1 += "整";
  return num1;
}

</script>
