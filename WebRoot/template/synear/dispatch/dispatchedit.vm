	       document.getElementById("taxmoney"+i).value = "";
	       return false;
	    }
	    if(isNaN(document.getElementById("taxmoney"+i).value)){
	       alert("目的客户含税金额必须为数字");
	       document.getElementById("taxmoney"+i).select();
	       return false;
	    }
	    if(parseFloat(document.getElementById("taxmoney"+i).value) < 0){
	       alert("目的客户含税金额不能小于0");
               document.getElementById("taxmoney"+i).select();
	       return false;
	    }
	    document.getElementById("hmoney"+i).value =(parseFloat(document.getElementById("taxmoney"+i).value)/parseFloat(document.getElementById("nnumber"+i).value)).toFixed(4);
	    document.getElementById("money"+i).value =(parseFloat(document.getElementById("hmoney"+i).value)/(1+parseFloat(document.getElementById("taxrate"+i).value)/100)).toFixed(4) ;
	    document.getElementById("notaxmoney"+i).value = (parseFloat(document.getElementById("money"+i).value) * parseFloat(document.getElementById("nnumber"+i).value)).toFixed(2);
	    document.getElementById("taxmny"+i).value = (parseFloat(document.getElementById("taxmoney"+i).value) - parseFloat(document.getElementById("notaxmoney"+i).value)).toFixed(2);
	 }
