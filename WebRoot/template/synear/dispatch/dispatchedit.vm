		}
	   }
    }
    function changetaxmoney(i,invname,custname){
     if(document.getElementById("taxmoney"+i).value !=""){
	    if(custname == ""){
	       alert("目的客户不能为空");
	       document.getElementById("taxmoney"+i).value = "";
	       return false;
	    }
	    if(invname == ""){
