<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<style type="text/css">
<!--
.STYLE1 {color: #FF0000}
-->
</style>
<title>=======</title>
<script language="javascript">
	function ShowDate(idText){
	 var state="dialogHeight: 300px; dialogWidth: 240px; dialogTop: 200px; dialogLeft: 300px; "+             "edge: Raised;  center: Yes; help: No; resizable: Yes; status: No;";
	 var reValue=window.showModalDialog('../../../js/date.html?OpenForm',"myDate",state);
	 if(reValue==null) 
	   ;
	 else
	   idText.value=reValue;
	 }
	function showtr(){
	var a = document.all.roles.value
		if(a=="R8905"){
		document.getElementById("tr").style.display="none";
		}
	}
</script>
</head>

<body onload="showtr()">
<form name="form1" action="PurchaseQualityTopdetail.aspx" method="post">
<table class="datatable">
  <tr class="trcolor">
    <td colspan="4" align="left">到货单查询</td>
  </tr>
  <tr class="trcolor">
    <td align="right">到货单日期:</td>
    <td align="left"><input type="text" name="TextZhiDanRiQi1y1" onclick="ShowDate(TextZhiDanRiQi1y1)" value="$!form_TextZhiDanRiQi1y1"/>-<input type="text" name="TextZhiDanRiQi1y2" onclick="ShowDate(TextZhiDanRiQi1y2)" value="$!form_TextZhiDanRiQi1y2"/></td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
  <input type="hidden" name="roles" value="$!session_ROLES">
  <tr class="trcolor" id="tr">
    <td>厂区:</td>
    <td align="left"><select name="SelectBuMen" disabled>
    <option value="">所有厂区</option>
    #set($i=0)
    #foreach($!m in $selFactory)
    <option value="$!m.PK_DEPTDOC" #if($form_SelectBuMen==$!m.PK_DEPTDOC) selected #end>$!m.DEPTNAME</option>
    #set($i=$i+1)
    #end
    </select></td>
    <td>仓库:<select name="SelectCangKu" disabled>
    <option value="">请选择仓库</option>
    #set($i=0)
    #foreach($!m in $selcangku)
    <option value="$!m.PK_STORDOC">$!m.STORNAME</option>
    #set($i=$i+1)
    #end
      </select></td>
    <td>&nbsp;</td>
  </tr>
  <tr class="trcolor">
    <td colspan="4"><input type="submit" class="bt1" name="Submit" value="查&nbsp;&nbsp;询" />&nbsp;&nbsp;<input type="Button" class="bt1" name="Submit" value="刷&nbsp;&nbsp;新" onclick="document.location.reload()"/></td>
  </tr>
</table>
</form>
<table class="datatable">
  <tr class="trcolor">
    <td>供应商号</td>
    <td>供应商名字</td>
    <td>订单号</td>
    <td>订单时间</td>
    <td>请购单号</td>
    <td>请购时间</td>
    <td>到货单号</td>
    <td>关联单据</td>
    <td>到货时间</td>
    <td>质检结果</td>
    <td>质检时间</td>
  </tr>
  #set($i=0)
  #foreach($!m in $selPurchaseQuality)
  <tr class="trcolor">
    <td>$!m.CUSTCODE</td>
    <td>$!m.CUSTNAME</td>
    <td>$!m.VORDERCODE</td>
    <td>$!m.DORDERDATE</td>
    <td>#if($!m.VPRAYCODE!="")$!m.VPRAYCODE#else&nbsp;#end</td>
    <td>#if($!m.DPRAYDATE!="")$!m.DPRAYDATE#else&nbsp;#end</td>
    <td><a #if($!m.QUALITYDATE!='*') class="STYLE1" #end href="PurchaseQuality.aspx?varrordercode=$m.VARRORDERCODE&pk_corp=$!m.PK_CORP">$!m.VARRORDERCODE</a><br>#if($!m.IBILLSTATUS!='3')<a #if($!m.QUALITYDATE!='*') class="STYLE1" #end href="PurchaseQuality2.aspx?varrordercode=$m.VARRORDERCODE&pk_corp=$!m.PK_CORP">质检</a>&nbsp;#else #end
    </td>
    <td>&nbsp;</td>
    <td>
    #if($!m.VDEF10!='')
    $!m.VDEF10
    #else
    &nbsp;
    #end
    </td>
    <td>#if($!m.QUALITYDATE!='*') 已质检
        #else &nbsp; 
	#end
	</td>
    <td>
    #if($!m.QUALITYDATE!='*')
    $!m.QUALITYDATE
    #else
    &nbsp;
    #end
    </td>
  </tr>
  #set($i=$i+1)
  #end
</table>
</body>
</html>