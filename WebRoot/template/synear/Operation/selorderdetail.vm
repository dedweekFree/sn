<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<title>==========</title>
</head>

<body>
<table class="datatable">
	#foreach($!m in $seloermaorderhead)
  <tr class="trcolor">
    <td>订单号:</td>
    <td align="left">$!m.ORDERCODE</td>
    <td>导入时间:</td>
    <td align="left">$!m.TS</td>
  </tr>
  <tr class="trcolor">
    <td>导入人:</td>
    <td align="left">$!m.USERNAME</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
	#end
  <tr class="trcolor">
    <td>&nbsp;</td>
    <td><input type="button" name="bt" class="bt1" value="返&nbsp;&nbsp;回" onclick="javascript:history.go(-1)"></td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
</table>
<br>
<table class="datatable">
  <tr class="trcolor">
    <td>序号</td>
    <td>产品名称</td>
    <td>连锁店名称</td>
    <td>门店号</td>
    <td>数量</td>
    <td>规格</td>
    <td>条码</td>
    <td>价格</td>
  </tr>
  #set($i=1)
  #foreach($!m in $selcustorderbody)
  <tr class="trcolor">
    <td>$i</td>
    <td>$!m.INVNAME</td>
    <td>$!m.STORENAME</td>
    <td>$!m.STORE</td>
    <td>$!m.QTY</td>
    <td>$!m.INSPEC</td>
    <td>#if($!m.TIAOMA!="")$!m.TIAOMA#else&nbsp;#end</td>
    <td>$!m.PRICE</td>
  </tr>
  #set($i=$i+1)
  #end
</table>
</body>
</html>
