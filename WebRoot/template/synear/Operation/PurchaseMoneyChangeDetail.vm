    <td>
    #if($!m.VDEF8!='')
    $!m.VDEF8
    #else
     0
    #end
    </td>
    <td>$!m.USER_NAME</td>
    <td>
    #if($!m.VNOTE!='')
    $!m.VNOTE
    #else
     0
    #end
    </td>
  </tr>
  #set($i=$i+1)
  #end
</table>
