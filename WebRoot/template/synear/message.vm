	<script type="text/javascript">
	    //mTarget转向目标窗口，空为不作处理
      var message="$!app_message";
	var mTarget="$!app_target";
	var mUrl="$!app_url";
        if(mTarget==""){
          alert(message);
	}else{
          if(mUrl==""){
            mUrl=top.oldurl;
	  }
	  if(!mUrl.haveChar(")"))
	   mUrl=mTarget+".location.replace('"+mUrl+"$!form_querystr')";
	  alert(message);
          eval(mUrl);	
	}

	</script>
