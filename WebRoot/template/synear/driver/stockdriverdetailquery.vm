<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<script  src="../../inc/js/treePicker.js" language="javascript"></script>
<script src="../../inc/js/pubjs.js"></script>
<script src="../../inc/js/preAfter.js"></script>
<link rel='StyleSheet' href='../../js/dtree.css' type='text/css' />
<title>=====</title>
<script language="javascript">
var nnum1 = 0;
</script>
</head>

<body>
<br>
<table class="datatable">
#foreach($!m in $snstocksendmanagerhquery)
  <tr class="trcolor">
    <td>调度单号：$!m.SENDID</td>
    <td>车牌号：$!m.DRIVERNUMBER</td>
    <td>联系电话：$!m.PHONE</td>
    <td>单据日期：$!m.CREATEDATE</td>
    <td>预约装车时间：$!m.ORDERTIME</td>
    <td>装车地点：#if($!m.ADDRESSNAME=='chenzhai')陈寨#elseif($!m.ADDRESSNAME=='sinian')思念#end</td>
  </tr>
  #end
</table>
<br>
<table class="datatable">
  <tr class="trcolor">
    <td>序号</td>
    <td>订单号</td>
    <td>数量</td>
    <td>到货地</td>
    <td>车型</td>
    <td>运输公司</td>
  </tr>
  #set($i=1)
  #foreach($!m in $snstocksendmanagerdquery)
  <tr class="trcolor">
    <td>$i</td>
    <td>$!m.ORDERCODE</td>
    <td>$!m.NNUMBER</td>
    <td>$!m.RECEIPTADDRESS</td>
    <td>$!m.DRIVERTYPE</td>
    <td>$!m.DRIVERCORP</td>
  </tr>
  #set($i=$i+1)
  <script>nnum1+=$!m.NNUMBER;</script>
  #end
  <tr class="trcolor">
       <td colspan="2">合计</td>
       <td><script language="javascript">document.write(nnum1);</script></td>
       <td colspan="3">&nbsp;</td>
  </tr>
  <tr>
     <td colspan="6" align="center"><input type="button" value="返回" class="bt1" onclick="javascript:history.back(-1)"></td>
  </tr>
</table>
</body>
</html>