<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<title>客户付款查询</title>
<script language="javascript">
   function ShowDate(idText){
	 var state="dialogHeight: 300px; dialogWidth: 240px; dialogTop: 200px; dialogLeft: 300px; "+             "edge: Raised;  center: Yes; help: No; resizable: Yes; status: No;";
	 var reValue=window.showModalDialog('../../../js/date.html?OpenForm',"myDate",state);
	 if(reValue==null) 
	   ;
	 else
	   idText.value=reValue;
    }
</script>
</head>
<body>
<form name="thisform" method="post" action='custpayquery.aspx'>
   <table class="datatable">
	<tr class="trcolor">
            <td>付款开始时间</td>
	    <td align="left"><input type="text" name="accountime" onclick="ShowDate(accountime)" value="$!form_accountime" /></td>
	    <td>付款截止时间</td>
	    <td align="left"><input type="text" name="endaccountime" onclick="ShowDate(endaccountime)" value="$!form_endaccountime" /></td>
	</tr>
	<tr class="trcolor">
            <td>客户提交开始时间</td>
	    <td align="left"><input type="text" name="orderaccountime" onclick="ShowDate(orderaccountime)" value="$!form_orderaccountime" /></td>
	    <td>客户提交截止时间</td>
	    <td align="left"><input type="text" name="orderendaccountime" onclick="ShowDate(orderendaccountime)" value="$!form_orderendaccountime" /></td>
	</tr>
        <tr class="trcolor">
	<td>是否付款</td>
	    <td align="left"><select name="payflag">
	        <option value=""></option>
		<option value="1" #if($!form_payflag == '1')selected#end>已付</option>
                <option value="0" #if($!form_payflag == '0')selected#end>未付</option>
	    </select></td>
	    <td>&nbsp;</td>
            <td>&nbsp;</td>
	</tr>
	<tr class="trcolor">
		<td colspan="4"><input class="bt1" type="submit" name="button" value="查&nbsp;&nbsp;询"></td>
	</td>
   </table>
</form>
<table class="datatable" cellspacing=0 cellpadding=0 border=0>
   <tr class="trcolor">
	<td>客户交易编号</td>
	<td>交易金额</td>
	<td>付款方账户</td>
	<td>付款方账户名</td>
	<td>付款时间</td>
	<td>客户提交时间</td>
	<td>收款方账户</td>
	<td>收款方账户名</td>
	<td>收款方账户开户行联行号</td>
	<td>是否付款</td>
   </tr>
   #foreach($m in $getcustpaymoneyquery)
   <tr class="trcolor">
	<td>$m.ORDERNO</td>
	<td>#if($m.TRNXAMOUNT != '')$m.TRNXAMOUNT#else&nbsp;#end</td>
	<td>#if($m.PAYACCOUNT != '')$m.PAYACCOUNT#else&nbsp;#end</td>
	<td>#if($m.PAYACCOUNTNAME != '')$m.PAYACCOUNTNAME#else&nbsp;#end</td>
	<td>#if($m.TRNXTIME != '')$m.TRNXTIME#else&nbsp;#end</td>
	<td>$m.TS</td>
	<td>$m.GETACCOUNT</td>
	<td>$m.GETACCOUNTNAME</td>
	<td>$m.GETACCOUNTNO</td>
	<td>#if($m.TRNXSTATUS == '0')未付#else已付#end</td>
   </tr>
   #end
   <tr >
        <td class="tdlabel td" colspan="10">$pagecode</td>
   </tr>
</table>
<form name="form1" method="post"></form>
<iframe name="saveframe" src="about:blank" width="100%" height="0" noresize frameborder=0 framespacing=0 marginheight=0 marginwidth=0></iframe> 
</body>
</html>