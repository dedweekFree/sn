<SCRIPT LANGUAGE="JavaScript">
   var prod=new Array(); 
   data=new data1('品名','批号','代码','规格','单位','数量');
   prod.add(data);
   #foreach($m in $sql1)
      data=new data1("$!m.INVNAME","$!m.VBATCHCODE","$!m.INVCODE","$!m.INVTYPE","$!m.UNIT","$!m.OUTCOUNT");
      prod.add(data);
   #end
</script>
