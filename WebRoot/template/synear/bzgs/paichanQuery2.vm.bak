        <td nowrap="nowrap" class="FixedTitleColumn">实际产量(件)</td>
	<td nowrap="nowrap" class="FixedTitleColumn">实际产量(吨)</td>
	<td nowrap="nowrap">计划完成率</td>
	<td nowrap="nowrap">原因分析</td>
</tr>
    #set($i=1) 
    #foreach($m in $getPaiChanDetail)
    
  		<tr height="20" class="trcolor">
			
		     <td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.BILLDATE</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.INVCLASSNAME</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.INVNAME</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.INVSPEC</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.UNITWEIGHT</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.PLANNUM</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.NINNUM</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.TON</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.ATIO</td>
			<td align="center" nowrap="nowrap" class="FixedDataColumn">$!m.REALMEMO</td>
		 </tr>
		 #set($i=$i+1)
   #end  
   #if($i==1)
     <tr height="20" bgcolor="#eeeeee" class="trcolor">
     <td colspan="9" bgcolor="#eeeeee" align="center" class="trcolor">没有相关信息</td>
     </tr>
   #end
</table>
