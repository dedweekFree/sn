<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<script  src="../../inc/js/treePicker.js" language="javascript"></script>
<script src="../../inc/js/pubjs.js"></script>
<script src="../../inc/js/preAfter.js"></script>
<link rel='StyleSheet' href='../../js/dtree.css' type='text/css' />
<title>=======</title>
<script language="javascript">
	function ShowDate(idText){
	 var state="dialogHeight: 300px; dialogWidth: 240px; dialogTop: 200px; dialogLeft: 300px; "+             "edge: Raised;  center: Yes; help: No; resizable: Yes; status: No;";
	 var reValue=window.showModalDialog('../../../js/date.html?OpenForm',"myDate",state);
	 if(reValue==null) 
	   ;
	 else
	   idText.value=reValue;
	 }
	    function setVals(dm,mc,code) {
	      setVal(dm,mc);
	      document.thisform.ptype.value=code;
	      document.thisform.ptypename.value=mc;
	 }
</script>
</head>

<body>
<form name="thisform" action="PurchaseQualityStatisticDetail.aspx" method="post">
<table class="datatable" id="tab">
  <tr class="trcolor">
    <td colspan="2" align="left">质检统计</td>
  </tr>
  <tr class="trcolor">
    <td>统计日期:</td>
    <td align="left">
      <input type="text" name="TextZhiDanRiQi1y1" onclick="ShowDate(TextZhiDanRiQi1y1)" value="$!form_TextZhiDanRiQi1y1"/>-<input type="text" name="TextZhiDanRiQi1y2" onclick="ShowDate(TextZhiDanRiQi1y2)" value="$!form_TextZhiDanRiQi1y2"/></td>
  </tr>
  <tr class="trcolor">
    <td>产品系列:</td>
    <td align="left"><input type="text" name="ptypename" value=""  readonly>
				<input type="hidden" name="ptype" value="">
				<input type='button' name=bt class='btn' value='选择' onclick="click_on_tree_selector($F('ptypename'),$F('dm_zzjg_tree_layer'))">
				<div class="dtree" id="dm_zzjg_tree_layer" style="position:absolute;width:300px;height:208px;visibility:Hidden;z-index:1"> 
				<table width="100%" border="1" cellspacing="0" cellpadding="0" height="100%"><tr><td>
					<IFRAME style="width:100%;height:100%" id='stree' name="swjgdm_tree_frame" marginWidth=0 marginHeight=0 src="class_tree.aspx" frameBorder=0 
						width="100%" height="100%" scrolling=auto BORDERCOLOR="#000000" ></IFRAME>
				</td></tr></table>
			        </div>
			        <script>
				   selector_layers[selector_layers.length]=document.all.dm_zzjg_tree_layer;
			        </script>
    </td>
  </tr>
  <tr class="trcolor">
    <td>入库类型:</td>
    <td align="left"><select name="instroetype">
      <option value="">无</option>
      <option value="258">合格</option>
      <option value="259">拒收</option>
      <option value="260">让步接受</option>
      </select></td>
  </tr>
  <tr class="trcolor">
    <td colspan="2"><input type="submit" class="bt1" name="Submit" value="查询" /></td>
  </tr>
</table>
</form>
<table class="datatable">
  <tr class="trcolor">
    <td>到货单号</td>
    <td>供应商</td>
    <td>品名</td>
    <td>数量</td>
    <td>质量类型</td>
    <td>质量备注</td>
    <td>入库类型</td>
  </tr>
  #set($i=0)
  #foreach($!m in $selPurchaseQualityStatistic)
  <tr class="trcolor">
    <td>$!m.VARRORDERCODE</td>
    <td>$!m.CVENDORNAME</td>
    <td>$!m.CBASENAME</td>
    <td>$!m.NARRVNUM</td>
    <td>
    #if($!m.TXT!="")
    $!m.TXT
    #else
    &nbsp;
    #end
    </td>
    <td>
        #if($!m.QUALITYMOTO!="")
    $!m.QUALITYMOTO
    #else
    &nbsp;
    #end
    </td>
    <td>#if($!m.TXT!="")
    $!m.TXT
    #else
    &nbsp;
    #end</td>
  </tr>
  #set($i=$i+1)
  #end
</table>
</body>
</html>