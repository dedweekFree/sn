    <td>$!m.VDEF4</td>
  </tr>
  #set($i=$i+1)
  #end
  <tr class="trcolor">
    <td colspan="4">供应商签字:___________________________</td>
    <td colspan="8" align="left">到&nbsp;货&nbsp;时&nbsp;间:&nbsp;_________年_________月_________日_________时_________分</td>
  </tr>
  <tr class="trcolor">
    <td colspan="4" rowspan="2">库管员签字:___________________________</td>
    <td colspan="8" align="left">卸车开始时间:_________年_________月_________日_________时_________分</td>
  </tr>
  <tr class="trcolor">
    <td colspan="8" align="left">卸车结束时间:_________年_________月_________日_________时_________分</td>
  </tr>
  <tr class="trcolor">
    <td colspan="4" rowspan="2">质检员签字:___________________________</td>
    <td colspan="8" align="left">检验开始时间:_________年_________月_________日_________时_________分</td>
  </tr>
  <tr>
    <td colspan="8" align="left">检验结束时间:_________年_________月_________日_________时_________分</td>
  </tr>
  <tr class="trcolor">
    <td colspan="12">
      <input name="b2" type="button" class="bt1" value="导出EXCEL表" onclick="javascript:method1('table1');"/>
      <input type="button" name="b4" class="bt1" value="返回" onclick="javascript:history.go(-1)"/></td>
  </tr>
</table>
</form>
</body>
</html>
<script>
function method1(tableid) {//整个表格拷贝到EXCEL中 
    var curTbl = document.getElementById(tableid); 
    var oXL = new ActiveXObject("Excel.Application"); 
    //创建AX对象excel 
    var oWB = oXL.Workbooks.Add(); 
    //获取workbook对象 
        var oSheet = oWB.ActiveSheet; 
    //激活当前sheet 
    var sel = document.body.createTextRange(); 
    sel.moveToElementText(curTbl); 
    //把表格中的内容移到TextRange中 
    sel.select(); 
    //全选TextRange中内容 
    sel.execCommand("Copy"); 
    //复制TextRange中内容  
    oSheet.Paste(); 
    //粘贴到活动的EXCEL中       
    oXL.Visible = true; 
    //设置excel可见属性 
} 
</script>