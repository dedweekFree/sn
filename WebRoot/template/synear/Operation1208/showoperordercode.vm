    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
    #end
  <tr class="trcolor">
    <td>存货编码</td>
    <td>存货名称</td>
    <td>单位</td>
    <td>需求日期</td>
    <td>订单数量</td>
    <td>备货次数</td>
    <td>备货数量</td>
    <td>计划到货日期</td>
    <td>备注</td>
    <td>到货仓库</td>
