<html>
<head>
    <title>======</title>
    <link rel="stylesheet" type="text/css" href="../../../js/ZhrCss.css">
    <meta http-equiv="Content-Type" content="text/html; charset=gb2312">

    <script src="../../../js/sunnyfunc.js"></script>

    <script src="../../../js/string.js"></script>

    <script>
var undefined;

function showtr(k,obj) { //v6.0
  if(obj.innerText=="查看"){
    obj.innerText="隐藏"
    tb.rows[k*2+2].style.display="";
  }else{
    obj.innerText="查看"
    tb.rows[k*2+2].style.display="none";
  }
}

function init(){
  try{
     if(dtb.length==undefined){
       tb.rows[2].style.display="none";
     }else{
       for(i=0;i<dtb.length;i++)
         tb.rows[i*2+2].style.display="none";
     }
  }
  catch(e){}
}

    </script>

</head>
<body onload="init()">
    <div class="Main">
        <div class="CaptionTxt">
            区域费用申请表</div>
        <%--    <table width="90%" border="0" align="center" cellpadding="0" cellspacing="0">
        <tr>
            <td height="30" class="fCopy" id="help">
            </td>
        </tr>
    </table>--%>
        <table id="tb" class="List">
            <tr class="trtitle" bgcolor="abcdef">
                <th>
                    区域
                </th>
                <th>
                    预算时间
                </th>
                <th>
                    状态
                </th>
                <th>
                    预算客户数
                </th>
                <th>
                    已报客户数
                </th>
                <th>
                    预算销售
                </th>
                <th>
                    预算应收
                </th>
                <th>
                    预算其它
                </th>
                <th>
                    预算产品费用
                </th>
                <th>
                    预算费用合计
                </th>
                <th>
                    申报销售
                </th>
                <th>
                    申报应收
                </th>
                <th>
                    申报其它
                </th>
                <th>
                    申报产品费用
                </th>
                <th>
                    申报费用合计
                </th>
                <th>
                    操作
                </th>
            </tr>
            #set($i=0) #foreach($m in $ZhrFeeRequestList)
            <tr class="trcolor4">
                <td align="center" nowrap>
                    $!m.DEPTNAME
                </td>
                <td align="center" nowrap>
                    $!m.APPLYMONTH
                </td>
                <td align="center" nowrap>

                    <script>'$!m.STATUS2'.toRealText('F','报批','被退回').write()</script>

                </td>
                <td align="center" nowrap>
                    $!m.ANUM
                </td>
                <td align="center" nowrap>
                    $!m.RNUM
                </td>
                <td align="center" nowrap>
                    $!m.PRE_SALEAIM
                </td>
                <td align="center" nowrap>
                    $!m.PRE_RECEVALAIM
                </td>
                <td align="center" nowrap>
                    $!m.PRE_ORTHERAIM
                </td>
                <td align="center" nowrap>
                    $!m.PRE_PRODFEE
                </td>
                <td align="center" nowrap>
                    $!m.PRE_ALLFEE
                </td>
                <td align="center" nowrap>
                    $!m.PRE_SALEAIM2
                </td>
                <td align="center" nowrap>
                    $!m.PRE_RECEVALAIM2
                </td>
                <td align="center" nowrap>
                    $!m.PRE_ORTHERAIM2
                </td>
                <td align="center" nowrap>
                    $!m.PRE_PRODFEE2
                </td>
                <td align="center" nowrap>
                    $!m.PRE_ALLFEE2
                </td>
                <td align="center" nowrap>
                    <a href="BudgetRequestAuditSubmit.aspx?applymonth=$!m.APPLYMONTH&areacode=$!m.AREACODE&status=F&step=1&oldstep=$!m.STEP2&remark="
                        target="dataframe">通过</a>&nbsp;&nbsp;<a href="BudgetRequestAuditSubmit.aspx?applymonth=$!m.APPLYMONTH&areacode=$!m.AREACODE&status=R&step=-1&oldstep=$!m.STEP2&remark="
                            target="dataframe">退回</a>&nbsp;&nbsp;<a href="BudgetRequestAuditDetail.aspx?applymonth=$!m.APPLYMONTH&areacode=$!m.AREACODE&status=$!m.STATUS2&step=$!m.STEP2&flow=2">明细</a>
                </td>
            </tr>
            <tr class="trcolor4">
                <td colspan="9" style="padding: 0px" id="dtb">
                </td>
            </tr>
            #set($i=$i+1) #end #if($i==0)
            <tr height="20" bgcolor="#eeeeee" class="trcolor">
                <td colspan="16" bgcolor="#eeeeee" align="center" class="trcolor">
                    没有相关信息
                </td>
            </tr>
            #end
        </table>
        <div class="Control">
            <input type="button" class="login2" value=" 刷  新 " name="B222" onclick="javascript:window.location.reload();">
        </div>
        <iframe name="dataframe" src="" width="0" height="0"></iframe>
    </div>
</body>
</html>
