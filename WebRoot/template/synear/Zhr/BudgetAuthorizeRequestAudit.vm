<html>
<head>
    <title>======</title>
    <link rel="stylesheet" type="text/css" href="../../../js/ZhrCss.css">
    <meta http-equiv="Content-Type" content="text/html; charset=gb2312">
<script src="../../../js/sunnyfunc.js"></script>
<script src="../../../js/string.js"></script>


    <script>
var undefined;

function showtr(k,obj) { //v6.0
  if(obj.innerText=="查看"){
    obj.innerText="隐藏"
    tb.rows[k*2+2].style.display="";
  }else{
    obj.innerText="查看"
    tb.rows[k*2+2].style.display="none";
  }
}

function init(){
  try{
     if(dtb.length==undefined){
       tb.rows[2].style.display="none";
     }else{
       for(i=0;i<dtb.length;i++)
         tb.rows[i*2+2].style.display="none";
     }
  }
  catch(e){}
}

    </script>

</head>
<body onload="init()">
    <div class="Main">
        <div class="CaptionTxt">
            客户费用申请表</div>
<%--    <table class="List">
        <tr>
            <td height="30" class="fCopy" id="help">
            </td>
        </tr>
    </table>--%>
    <table id="tb"  class="List">
        <tr class="trtitle" bgcolor="abcdef">
            <th width="8%" height="25">
                区域
            </th>
            <th width="8%" height="25">
                预算时间
            </th>
            <th width="8%" height="25">
                状态
            </th>
            <th width="8%" height="25">
                分管客户数
            </th>
            <th width="8%" height="25">
                已报客户数
            </th>
            <th width="8%" height="25">
                销售目标
            </th>
            <th width="8%" height="25">
                应收目标
            </th>
            <th width="8%" height="25">
                其它目标
            </th>
            <th width="8%" height="25">
                产品费用预算
            </th>
            <th width="8%" height="25">
                费用预算合计
            </th>
            <th height="25">
                操作
            </th>
        </tr>
        #set($i=0) #foreach($m in $ZhrFeeApplyAuditList)
        <tr class="trcolor4">
            <td align="center">
                $!m.DEPTNAME
            </td>
            <td align="center">
                $!m.APPLYMONTH
            </td>
            <td align="center">

                <script>'$!m.STATUS'.toRealText('F','报批','被退回').write()</script>

            </td>
            <td align="center">
                $!m.ANUM
            </td>
            <td align="center">
                $!m.RNUM
            </td>
            <td align="center">
                $!m.PRE_SALEAIM
            </td>
            <td align="center">
                $!m.PRE_RECEVALAIM
            </td>
            <td align="center">
                $!m.PRE_ORTHERAIM
            </td>
            <td align="center">
                $!m.PRE_PRODFEE
            </td>
            <td align="center">
                $!m.PRE_ALLFEE
            </td>
            <td align="center">
                <a href="BudgetAuthorizeRequestAuditSubmit.aspx?applymonth=$!m.APPLYMONTH&areacode=$!m.AREACODE&status=F&step=1&oldstep=$!m.STEP&remark="
                    target="dataframe">通过</a>&nbsp;&nbsp;<a href="BudgetAuthorizeRequestAuditSubmit.aspx?applymonth=$!m.APPLYMONTH&areacode=$!m.AREACODE&status=R&step=-1&oldstep=$!m.STEP&remark="
                        target="dataframe">退回</a>&nbsp;&nbsp;<a href="BudgetAuthorizeRequestAuditDetail.aspx?applymonth=$!m.APPLYMONTH&areacode=$!m.AREACODE&status=$!m.STATUS&step=$!m.STEP">明细</a>
            </td>
        </tr>
        <tr class="trcolor4">
            <td colspan="8" style="padding: 0px" id="dtb">
            </td>
        </tr>
        #set($i=$i+1) #end #if($i==0)
        <tr height="20" bgcolor="#eeeeee" class="trcolor">
            <td colspan="11" bgcolor="#eeeeee" align="center" class="trcolor">
                没有相关信息
            </td>
        </tr>
        #end
    </table>
    <div class="Control">
        <input type="button" class="login2" value=" 刷  新 " name="B222" onclick="javascript:window.location.reload();">
    </div>
    <iframe name="dataframe" src="" width="0" height="0"></iframe>
    </div>
</body>
</html>
