<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<link rel="stylesheet" type="text/css" media="all" href="../../../js/calendar-win2k-1.css"/> 
<script src="../../../inc/js/pubjs.js"></script>
<script src="../../../inc/js/preAfter.js"></script>
<script type="text/javascript" src="../../../inc/js/calendar.js"></script>
<script type="text/javascript" src="../../../inc/js/calendar-setup.js"></script>
<script type="text/javascript" src="../../../inc/js/calendar-en.js"></script>
<script type="text/javascript" src="../../../inc/js/calendarnew.js"></script>
<SCRIPT LANGUAGE="JavaScript">
function checkinput()
{  
  thisform.page.value=1;
  thisform.submit();
}
</SCRIPT>
<title>投标状态</title>
</head>

<body>
<table border="0" cellspacing="0" cellpadding="0" width="100%" align="center">
<tr>
  <form name="thisform" method="post">
 
    <!--pageSize主要是完成分页功能，设置显示的记录的条数-->
   <input type="hidden" name="pageSize" value="$!form_pageSize">
   <input type="hidden" name="page" value="">
   <td colspan=2>
      <table border="0" cellspacing="1" cellpadding="0" bgcolor="#ffffff" width="98%" align="center"  class="datatable">

		<tr class="trcolor">
		    <td colspan="4" align="left">条件查询</td>
		  </tr>
		  <tr class="trcolor">
		    <td>项目ID:</td>
		    <td align="left"><input type="text" name="proid" value="$!form_proid"/></td>
		    <td>项目名称:</td>
		    <td align="left"><input type="text" name="proname" value="$!form_proname"/></td>
		  </tr>
		  <tr class="trcolor">
		    <td>项目状态:</td>
		    <td align="left"><select name="selstatus">
		    <option value=""></option>
		    <option value="K" #if($!form_selstatus=='K') selected #end>中标</option>
		    <option value="T" #if($!form_selstatus=='T') selected #end>进入谈判合同</option>
		    <option value="F" #if($!form_selstatus=='F') selected #end)>未中标</option>
		    </select></td>
		    <td>第几轮:</td>
		    <td align="left"><input type="text" name="num" value="$!form_num"/>
		    </td>
		  </tr>
		  <tr class="trcolor">
		     <td>开始时间:</td><td align="left"><input name="startTime" type="text" value="$!form_startTime" onfocus="calendar()"/></td>
		     <td>结束时间:</td><td align="left"><input name="endTime" type="text" value="$!form_endTime" onfocus="calendar()"/></td>
		  </tr>
		<tr>
		   <td align="center" colspan="4"><input type="button" name="submit1" value="搜 索" onclick="checkinput()" class="bt1">
				&nbsp;&nbsp;</td>
		</tr>
		<tr>
		  <td>
		  </td><td align="left"></td>
		</tr>
	  </table>
   </td></form>
</tr>
<tr><td>
<table border="0" cellspacing="0" cellpadding="0" bgcolor="#ffffff" width="98%" align="center" id="tb" 

class="datatable">
<tr height="20"  align="center" class="trtitle">
    <td>项目ID</td>
    <td>项目名称</td>
    <td>到岸地</td>
    <td>投标价</td>
    <td>投标量</td>
    <td>招标量</td>
    <td>单位</td>
    <td>投标开始时间</td>
    <td>投标结束时间</td>
    <td>状态</td>
    <td>第几轮</td>
</tr>

    #set($i=1) 
    #foreach($m in $filedown)
  		<tr class="trcolor">
		    <td>$!m.PROID</td>
		    <td>$!m.PRONAME</td>
		    <td>#if($!m.AREANAME == 'synear')&nbsp;#else$!m.AREANAME#end</td>
		    <td align="left">$!m.TENDPRICE元</td>
		    <td>$!m.QUANTITY</td>
		    <td>$!m.TENDQUANTITY</td>
		    <td>$!m.TYPENAME</td>
		    <td>$!m.ATIME</td>
		    <td>$!m.ZTIME</td>
		    #if($!m.STATUS == 'A')
			#if($!m.TENDSTATUS=='K') <td><font color="red">中标</font></td><td>第[$!m.TENDNUM]轮</td>
			#end
			#if($!m.TENDSTATUS=='T') <td><font color="red">进入谈判合同</td><td>第[$!m.TENDNUM]轮</td>
			#end
			#if($!m.TENDSTATUS=='F') <td>未中标</td><td>第[$!m.TENDNUM]轮</td>
			#end
		    #else
			 <td>
			    #if($!m.TENDSTATUS=='K') <font color="red">中标</font>#end
			    #if($!m.TENDSTATUS=='T') <font color="red">进入谈判合同</font>#end
			    #if($!m.TENDSTATUS=='F') 未中标#end
			 </td>
			    <td>第[$!m.TENDNUM]轮</td>
		    #end
		  </tr>
		 #set($i=$i+1)
   #end  
   #if($i==1)
     <tr height="20" bgcolor="#eeeeee" class="trcolor">
     <td colspan="9"  bgcolor="#eeeeee" align="center" class="trcolor">没有相关信息</td>
     </tr>
   #end
</table>
</td></tr>
<tr><td height=5></td></tr>
<tr><td>
		<table width="100%">
   <tr >
        <td class="tdlabel td">$pagecode</td>
   </tr>
</table>
</td></tr>
</table>

<br>
<iframe name="saveframe" src="about:blank" width="100%" height="0" noresize frameborder=0 framespacing=0 

marginheight=0 marginwidth=0></iframe> 
</body>
</html>
