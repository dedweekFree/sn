<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<link rel="stylesheet" type="text/css" href="../../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../../js/spellCode.css">
<title>供应商资料查询</title>
<script  src="../../../inc/js/treePicker.js" language="javascript"></script>
<script src="../../../inc/js/pubjs.js"></script>
<script src="../../../inc/js/preAfter.js"></script>
<SCRIPT LANGUAGE="JavaScript">
function checkinput()
{  
  thisform.page.value=1;
  thisform.submit();
}
function setVals(dm,mc,code) {
      setVal(dm,mc);
      thisform.ptypeid.value=dm;
      thisform.ptypename.value=mc;
      thisform.ptype.value=code;
    }
</SCRIPT>
</head>

<body>
<table class="datatable">
<form name="thisform" method="post" action="">
<input type="hidden" name="pageSize" value="$!form_pageSize">
   <input type="hidden" name="page" value="">
   <tr class="trcolor">
    <td colspan="5" align="left">供应商资料查询</td>
  </tr>
   <tr class="trcolor">
    <td>供应商编码:</td>
    <td align="left"><input type="text" name="username" value="$!form_username"/></td>
    <td>供应商名称:</td>
    <td align="left"><input type="text" name="company" value="$!form_company"/></td>
    <td>&nbsp;</td>
  </tr>
  <tr class="trcolor">
	    <td>产品分类:</td>
	    <td align="left"><input type="text" name="ptypename" value="$!form_ptypename" readonly>
	<input type="hidden" name="ptype" value="$!form_ptype">
	<input type="hidden" name="ptypeid" value="$!form_ptypeid">
	<input type='button' name=bt class="bt1" value='选择' onclick="click_on_tree_selector($F('ptypename'),$F('dm_zzjg_tree_layer'))">
	<div class="dtree" id="dm_zzjg_tree_layer" style="position:absolute;width:300px;height:208px;visibility:Hidden;z-index:1"> 
	<table width="100%" border="1" cellspacing="0" cellpadding="0" height="100%"><tr><td>
		<IFRAME style="width:100%;height:100%" name="swjgdm_tree_frame" marginWidth=0 marginHeight=0 src="class_tree.aspx" frameBorder=0 
			width="100%" height="100%" scrolling=auto BORDERCOLOR="#000000" ></IFRAME>
	</td></tr></table>
	</div>
	<script>
	   selector_layers[selector_layers.length]=document.all.dm_zzjg_tree_layer;
	</script>	</td>
	    <td>是否审核:</td>
	    <td align="left">
		<select type="text" name="ischeck" value="$!form_ischeck"/>
		     <option value=""></option>
		     <option value="B" #if($!form_ischeck == "B")selected#end>通过审核</option>
		     <option value="A" #if($!form_ischeck == "A")selected#end>待审核</option>
		     <option value="C" #if($!form_ischeck == "C")selected#end>退回</option>
		</select>
	    </td>
	    <td>&nbsp;</td>
	  </tr>
	<tr class="trcolor">
	    <td>添加人:</td>
	    <td align="left"><input type="text" name="addusername" value="$!form_addusername"/></td>
	    <td>&nbsp;</td>
	    <td align="left">&nbsp;</td>
	    <td>&nbsp;</td>
	  </tr>
	<tr>
	   <td align="center" colspan="5"><input type="button" name="submit1" value="搜 索" onclick="checkinput()" class="bt1">
			&nbsp;&nbsp;</td>
	</tr>
</form>
  <tr class="trcolor">
    <td>注册时间</td>
    <td>供应商编码</td>
    <td>供应商名称</td>
    <td>添加人</td>
    <td nowrap>是否通过审核</td>
  </tr>
  #set($i=0)
  #foreach($!m in $querytenduser)
  <tr class="trcolor">
    <td nowrap>$!m.MAKETIME</td>
    <td nowrap><a href="vendoredit.aspx?userid=$!m.USERID">$!m.USERNAME</a></td>
    <td nowrap>$!m.COMPANY</td>
    <td nowrap>#if($!m.ADDUSERNAME!="")$!m.ADDUSERNAME#else&nbsp;#end</td>
    <td nowrap>#if($!m.MAKESTATUS=='B')<font color='red'>是</font>#else否#end</td>
  </tr>
  #set($i=$i+1)
  #end
  <tr class="trcolor">
    <td colspan="5">      
      <input type="button" name="b2" value="刷&nbsp;&nbsp;新" class="bt1" onclick="javascript:location.reload()"/></td>
  </tr>
  <tr >
        <td class="tdlabel td" colspan="5">$pagecode</td>
   </tr>
</table>
    </div>
    <iframe name="dataframe" src="" width="0" height="0"></iframe>
    </div>
</body>
</html>
