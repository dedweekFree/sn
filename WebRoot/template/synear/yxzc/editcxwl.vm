<HTML>
<HEAD>		
<title>费用申请表</title>
<link rel="stylesheet" type="text/css" href="../../js/style.css">
<link rel="stylesheet" type="text/css" href="../../js/spellCode.css">
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">


<script  src="../../../inc/js/treePicker.js" language="javascript"></script>
<script Language="JavaScript" src="../../../include/js/PopCalendar.js"></script>
<script src="../../../inc/js/pubjs.js"></script>
<script src="../../../inc/js/preAfter.js"></script>

<SCRIPT LANGUAGE="JavaScript">
var bData1 = new Array(); 
bData1 = [['序号','sid','id']
#foreach($!m in $custcxwlalledit)
,['$i','$m.SID','$m.ID']
#end
]
function tosave(custcode){
   var datahtml="";
   for(var i=1;i<bData1.length;i++){
      datahtml+="<input type=hidden name=sid value='"+bData1[i][1]+"'>";
      datahtml+="<input type=hidden name=id value='"+bData1[i][2]+"'>";
      datahtml+="<input type=hidden name=snnum value='"+document.getElementById('snnum'+i).value+"'>";
   }
   subdata.innerHTML=datahtml;
   form2.target="saveframe";
   form2.action = "cxwlsavedit.aspx";
   form2.submit();
}
</SCRIPT>
</HEAD>
<BODY>
<form name="form1" method="post">
<table border="0" cellspacing="0" cellpadding="0" bgcolor="#ffffff" width="98%" align="center" class="datatable">
<tr height="20"  align="center" >
	<td nowrap="nowrap">发放物料时间</td>
	<td nowrap="nowrap">客户编码</td>
	<td nowrap="nowrap">客户名称</td>
	<td nowrap="nowrap">物料名称</td>
	<td nowrap="nowrap">计量单位</td>
	<td nowrap="nowrap">出库数量</td>
	<td nowrap="nowrap">实收数量</td>
	<td nowrap="nowrap">出库单价</td>
	<td nowrap="nowrap">出库金额（元）</td>
</tr>
    #set($i=1) 
    #foreach($m in $custcxwlalledit)
	<tr height="20" class="trcolor">
	     <td align="center" nowrap="nowrap">$!m.SENDDATE</td>
		<td align="center" nowrap="nowrap">$!m.CUSTCODE</td>
		<td align="center" nowrap="nowrap">$!m.CUSTNAME</td>
		<td align="center" nowrap="nowrap">$!m.WLNAME</td>
		<td align="center" nowrap="nowrap">$!m.UNITNAME</td>
		<td align="center" nowrap="nowrap">$!m.NNUM</td>
		<td align="center" nowrap="nowrap">
		   <input type="text" style="width:70" name="snnum$i" id="snnum$i" value="$!m.NNUM">
		</td>
		<td align="center" nowrap="nowrap">$!m.PRICE</td>
		<td align="center" nowrap="nowrap">$!m.MONEY</td>
	 </tr>
	 #set($i=$i+1)
   #end  
   #if($i==1)
     <tr height="20" bgcolor="#eeeeee" class="trcolor">
     <td colspan="9" bgcolor="#eeeeee" align="center" class="trcolor">没有相关信息</td>
     </tr>
   #end
</table>
</form>
<div align="center"><input type="button" class="login2" value="提交 " name="B222" onClick="tosave('$!form_custcode')">&nbsp;<input type="button" class="login2" value="返回 " name="B222" onClick="javascript:history.back(-1)"> </div>
<form name="form2" method="post">
  <span id="subdata"></span>
</form>

<iframe name="saveframe" src="about:blank" width="100%" height="0" noresize frameborder=0 framespacing=0 

marginheight=0 marginwidth=0></iframe> 
</BODY>
</HTML>
