

<html>
<head>
<link href="../../css.css" rel="stylesheet" type="text/css"><meta http-equiv="Content-Language" content="en-us">
<meta name="GENERATOR" content="Microsoft FrontPage 5.0">
<meta name="ProgId" content="FrontPage.Editor.Document">
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
<title></title>
<script language="JavaScript" src="../../../js/md5.js"></script>
<script language='javascript'>
   function  amentPw()
   {
      var pw = this.form1.pswd.value;
      var pw2 = this.form1.pswd2.value;
      if(this.form1.oldpwd.value == "")
      {
	 alert("原密码不能为空!");
	 return false;
      }
      #foreach($m in $getPassword)
      if(md5(this.form1.oldpwd.value) != "$m.PASS")
      {
	 alert("原密码错误!");
	 return false;
      }
      #end
      if(pw == "" || pw2 == "")
      {
	 alert("密码不能为空!");
	 return false;
      }
      if(pw != pw2)
      {
	    alert("两次密码不一致，请重新输入密码");
	    return false;
      }
      this.form1.password.value=md5(pw);
      this.form1.submit();
  }
</script>
</head>

<body>

<form  name='form1' method='post' action='setPasswordSubmit.aspx' target="dataframe">
<br><br><br>
<table width="50%" height="35"  border="0" align="center" cellpadding="0" cellspacing="0" class="B1">
  <tr>
    <td bgcolor="#00B000" class="f" align='center'>密码修改</td>
  </tr>
</table>
<table width="50%"  border="0" align="center" cellpadding="0" cellspacing="1" bordercolor="#CCCCCC" bgcolor="#CCCCCC">
  <tr align="center" valign="middle" bgcolor="#E3E3E3" class="fCopy">
    <td width="40%" height="25" align="right"> &nbsp;</td>
    <td align="left" colspan='2'> &nbsp;&nbsp;</td>
  </tr>
  <tr align="left" bgcolor="#FFFFFF" class="fCopy">   
    <td width="15%" height="25" >&nbsp;&nbsp;请输入原密码：</td>   
    <td colspan='2'><input type='password' name='oldpwd' value=''size='20'><input type='hidden' name='password' value=''></td>
  </tr> 
  <tr align="left" bgcolor="#FFFFFF" class="fCopy">   
    <td width="15%" height="25" >&nbsp;&nbsp;请输入新密码：</td>   
    <td colspan='2'><input type='password' name='pswd' value=''size='20'></td>
  </tr>   
  <tr align="left" bgcolor="#FFFFFF" class="fCopy">   
    <td width="15%" height="25">&nbsp;&nbsp;请再次输入新密码：</td>   
    <td colspan='2'><input type='password' name='pswd2' value=''size='20'></td>
  </tr> 
  <tr align="center" valign="middle" bgcolor="#FFFFFF" class="fCopy">
  </tr>
  <tr align="center" valign="middle" bgcolor="#FFFFFF" class="fCopy">
    <td height="25" colspan="3" align="right">&nbsp;</td>
  </tr>
  <tr align="center" bgcolor="#FFFFFF"class="fCopy">   
    <td width="15%" colspan='3'><input type='button' class='button' name='pw' value='修&nbsp;&nbsp;改' onClick='amentPw()'>
    <input type='reset' class='button' name='pw' value='重&nbsp;&nbsp;置'></td>
  </tr> 
</table>
</form>
<iframe name="dataframe" src="" width="0" height="0"></iframe>
</body>
</html>  