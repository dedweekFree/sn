<body>
<form name="form1" action="" method="post"><input type="hidden" name="baseid" value="$!form_baseid">
<table class="datatable">
  <tr class="trcolor">
    <td colspan="4" align="left">人员信息注册
    </td>
  </tr>
  #foreach($m1 in $selbasicuserinfobasic)
   <tr class="trcolor">
    <td align="right">用户名</td>
    <td align="left">
      <input type="text" name="name" value="$!m1.NAME" />
      <span class="STYLE1">*</span> </td>
    <td align="right">任职级别</td>
    <td align="left">
      <select name="level">
         <option value=""></option>
	 #foreach($m in $selbasiclevel)
           <option value="$!m.LEVELID" #if($!m.LEVELID == $!m1.LEVELID)selected#end>$!m.LEVELNAME</option>
	 #end
      </select>
      <span class="STYLE1">*</span> </td>
  </tr>
  <tr class="trcolor">
    
  </tr>
  <tr class="trcolor">
    <td align="right">大区</td>
    <td align="left">
      <select name=dq_pk  onchange="javascript:setF(this.value,'areapk');">
	<option value=''>请选择大区</option>
      #foreach($m2 in $selsyneardaqu)
	     <option value="$!m2.NC_PK" #if($m1.PK_DEPTDOC_FATHER==$m2.NC_PK) selected #end>$!m2.DEPTNAME</option>
      #end
	</select>	<span class="STYLE1">*</span> 
    </td>
    <td align="right">区域</td>
    <td align="left">
     <select name=areapk><option value=''>请选择区域</option>
      #foreach($m3 in $selsyneararea)
	     <option value="$!m3.NC_PK" #if($m1.PK_DEPTDOC==$m3.NC_PK) selected #end>$!m3.DEPTNAME</option>
      #end
     </select>
      </td>
  </tr>
  <tr class="trcolor">
    <td align="right">工商卡号</td>
    <td align="left">
      <input type="text" name="bankno" value="$!m1.BANKNUM"  onkeyup="if(isNaN(value))execCommand('undo')" onafterpaste="if(isNaN(value))execCommand('undo')"/><span class="STYLE1">*</span> 
    </td>
    <td align="right">常住地</td>
    <td align="left">
      <input type="text" name="addressname"  value="$!m1.ADDRESSNAME" /><input type="button" value="..." onclick="togetmont('diququery.aspx?F_policy=addressname')" class="bt1">
      <span class="STYLE1">*</span> </td>
  </tr>
  <tr class="trcolor">
    <td align="right">驻地天数</td>
    <td align="left">
      <input type="text" name="addressday"  value="$!m1.ADDRESSDAY"  /><span class="STYLE1">*</span> 
    </td>
    <td align="right">用户分配</td>
    <td align="left">
      <input type="text" name="feipeiuser" value="$!m1.USERNAME" readonly><input type="button" onclick="openewin('feipeiuserquery.aspx')" value="..." class="bt1"></td>
  </tr>
  <tr class="trcolor">
    <td align="right">手机号</td>
    <td align="left">
      <input type="text" name="phone" value="$!m1.PHONE"  onkeyup="if(isNaN(value))execCommand('undo')" onafterpaste="if(isNaN(value))execCommand('undo')"/><span class="STYLE1">*</span> 
    </td>
    <td align="right">身份证号</td>
    <td align="left">
      <input type="text" name="card"  value="$!m1.CARD"/>
      <span class="STYLE1">*</span> </td>
  </tr>
  <tr class="trcolor">
    <td align="right">家庭地址</td>
    <td align="left" colspan="3">
      <input type="text" name="addresshome" style="width:500" value="$!m1.ADDRESS" readonly /><input type="button" value="..." onclick="togetmont('diququery.aspx?F_policy=addresshome')" class="bt1"> <span class="STYLE1">*</span>
    </td>
  </tr>
  #end
  <tr class="trcolor">
    <td colspan="4">
      <input type="button" name="Submit2" class="bt1" value="提交" onclick="checksub()"/>
      <input type="button" name="Submit" onclick="javascript:history.back(-1)" class="bt1" value="返回" /></td>
  </tr>
  </table>
</form>
